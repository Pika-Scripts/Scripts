_G.ToggleColor = Color3.fromRGB(255,255,255)
_G.ButtonColor = Color3.fromRGB(255,255,255)

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Water1001/WallyUi3/main/.lua')))() -- It's obfuscated, I won't let you see my ugly coding skills. =)

local w = library:CreateWindow("Tower of Hell(Patched LOL)")

local a = w:CreateFolder("Main")

a:Button("Instant Win",function()
game.Players.LocalPlayer.Character.KillScript:Destroy()
wait(0.5)
local win = game:GetService("Workspace").tower.sections.finish.exit.ParticleBrick.CFrame

local ts = game:GetService("TweenService")

local tween = ts:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(5), {CFrame = win})
tween:Play()
 end)

 a:Button("God Mode",function()
     game.Players.LocalPlayer.Character.KillScript:Destroy()
      end)

      a:Button("Get All Tools",function()
          local copy = game:GetService("ReplicatedStorage").Gear["killpart"]:Clone()
                    
copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["bomb"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["jump"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["hook"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["speed"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["yxterminator"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["hourglass"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["gravity"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["trowel"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
                local copy = game:GetService("ReplicatedStorage").Gear["fusion"]:Clone()

copy.Parent = game.Players.LocalPlayer.Backpack
           end)


 

